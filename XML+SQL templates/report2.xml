<?xml version="1.0" encoding="UTF-8"?>
<report>
    <dbname metadata="metadata.xml">AppleShop</dbname>
    <visible>
        <rows>
            <table>Store</table>
            <column>Address</column>
        </rows>
        <columns>
            <table>Operation</table>
            <column>Date</column>
            <group>Month</group>
        </columns>
        <aggregate>
            <name>Amount</name>
        </aggregate>
    </visible>
    <fixeddata>
        <options>
            <table>Item</table>
            <column>Model</column>
            
            <option>iPhone 5s</option>
            <option>iPad mini</option>
        </options>
        <options>
            <table>Buyer</table>
            <column>Gender</column>
            
            <option>Female</option>
        </options>
        <fromto>
            <table>Operation</table>
            <column>Date</column>
            <from>2014-01-01</from>
            <to>2014-12-31</to>
        </fromto>
    </fixeddata>
</report>
