<?xml version="1.0" encoding="UTF-8"?>
<report>
    <dbName metadata="metadata.xml">AppleShop</dbName>
    <visible>
        <rows>
            <table>Store</table>
            <column>Address</column>
        </rows>
        <columns>
            <table>Operation</table>
            <column>Date</column>
            <group>Week</group>
        </columns>
        <aggregate>
            <name>Amount</name>
        </aggregate>
    </visible>
    <fixeddata>
        <restriction>
            <table>Item</table>
            <column>Model</column>
            <options>
                <option>iPhone</option>
                <option>iPad</option>
            <options>
        </restriction>
        <restriction>
            <table>Buyer</table>
            <column>Gender</column>
            <options>
                <option>Female</option>
            <options>
        </restriction>
        <fromto>
            <table>Operation</table>
            <column>Date</column>
            <from>2014-01-01</from>
            <to>2014-12-31</to>
        </fromto>
    </fixeddata>
</report>
