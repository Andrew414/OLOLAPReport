<?xml version="1.0" encoding="UTF-8"?>
<report>
    <dbname metadata="metadata.xml">AppleShop</dbname>
    <visible>
        <rows>
            <table>Buyer</table>
            <column>Gender</column>
        </rows>
        <columns>
            <table>Store</table>
            <column>City</column>
        </columns>
        <aggregate>
            <name>Price</name>
        </aggregate>
    </visible>
    <fixeddata>
        <restriction>
            <table>Item</table>
            <column>Model</column>
            <options>
                <option>iPhone</option>
            </options>
        </restriction>
    </fixeddata>
</report>
